{
   
    "source": {
        "include": ["./rc"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "docs/",
        "recurse": true,
        "readme": "./README.md",
        "verbose": true
    }
}